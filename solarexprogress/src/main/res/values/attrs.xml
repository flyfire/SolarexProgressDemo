<resources>
    <declare-styleable name="CommericalProgressbar">
        <attr name="cpb_background" format="color"/>
        <attr name="cpb_foreground" format="color"/>
        <attr name="cpb_max" format="integer"/>
        <attr name="cpb_progress" format="integer"/>
        <attr name="cpb_secondprogress" format="integer"/>
        <attr name="cpb_secondprogresscolor" format="color"/>
    </declare-styleable>
    <declare-styleable name="RectProgressbar">
        <attr name="rpb_background" format="color"/>
        <attr name="rpb_foreground" format="color"/>
        <attr name="rpb_max" format="integer"/>
        <attr name="rpb_progress" format="integer"/>
        <attr name="rpb_horizontalgap" format="integer"/>
        <attr name="rpb_verticalgap" format="integer"/>
        <attr name="rpb_rectperline" format="integer"/>
        <attr name="rpb_linenumber" format="integer"/>
    </declare-styleable>
    <declare-styleable name="BubbleProgressView">
        <attr name="bpv_progress" format="integer"/>
        <attr name="bpv_max" format="integer"/>
        <attr name="bpv_bubbleheight" format="integer"/>
        <attr name="bpv_bubblewidth" format="integer"/>
        <attr name="bpv_bordercolor" format="color"/>
        <attr name="bpv_backgroundcolor" format="color"/>
    </declare-styleable>
</resources>